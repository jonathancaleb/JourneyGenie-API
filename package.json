{
    "name": "cruise",
    "version": "1.0.0",
    "description": "Backend API for road transport app, similar to Uber",
    "main": "index.js",
    "scripts": {
        "lint": "eslint src --color",
        "build": "babel src --out-dir dist --ignore node_modules",
        "p": "prettier --write 'src/**/*.js'",
        "p:w": "onchange 'src/**/*.js' -- prettier --write {{changed}}",
        "test": "cross-env NODE_ENV=test mocha ./src/test/ --timeout 40000",
        "dev": "cross-env NODE_ENV=dev nodemon ./src/server.js",
        "prod": "cross-env NODE_ENV=prod node ./src/server.js",
        "start": "node ./src/server",
        "doc": "jsdoc -c jsdoc.json"
    },
    "engines":{
        "node": ">=14"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tech-bro-association/cruise.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Tech-bro-association/cruise/issues"
    },
    "homepage": "https://github.com/Tech-bro-association/cruise#readme",
    "dependencies": {
        "axios": "^1.2.1",
        "bcrypt": "^5.1.0",
        "bcryptjs": "^2.4.3",
        "buffer": "^6.0.3",
        "cloudinary": "^1.33.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.3",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-standard": "^5.0.0",
        "express": "^4.18.2",
        "express-async-error": "^0.0.2",
        "express-async-errors": "^3.1.1",
        "google-auth-library": "^8.5.1",
        "googleapis": "^105.0.0",
        "jsdoc": "^4.0.0",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.1.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.8.0",
        "nodemon": "^2.0.19",
        "socket.io": "^4.5.4",
        "sphinx-js": "^0.0.9",
        "supertest": "^6.3.0",
        "uuid": "^9.0.0",
        "validator": "^13.7.0",
        "websocket": "^1.0.34",
        "ws": "^8.12.0"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "chai": "^4.3.6",
        "eslint": "^8.29.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "mocha": "^10.1.0",
        "nodemon": "^2.0.20"
    },
    "prettier": {
        "printWidth": 500
    }
}
